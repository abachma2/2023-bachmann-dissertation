	keywords = {LWR, reprocessed uranium, fuel cycle, minor actinides, centrifuge, cost assessment, feasibility, proliferation resistance, protected plutonium production, re-enrichment},
	pages = {1016--1027},
	annote = {Publisher: Taylor \& Francis \_eprint: https://www.tandfonline.com/doi/pdf/10.1080/18811248.2008.9711887},
	file = {Full Text PDF:C\:\\Users\\Amanda\\Zotero\\storage\\3HQENQ72\\FUKUDA et al. - 2008 - Feasibility of Reprocessed Uranium in LWR Fuel Cyc.pdf:application/pdf;Snapshot:C\:\\Users\\Amanda\\Zotero\\storage\\LYTXMWUC\\18811248.2008.html:text/html},
