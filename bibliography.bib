	file = {ScienceDirect Full Text PDF:C\:\\Users\\Amanda\\Zotero\\storage\\SA6P2782\\Kim et al. - 1999 - Selection of an optimal nuclear fuel cycle scenari.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\Amanda\\Zotero\\storage\\BFI8GQB5\\Kim et al. - 1999 - Selection of an optimal nuclear fuel cycle scenari.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Amanda\\Zotero\\storage\\JS36TCCQ\\S0306454998000814.html:text/html;ScienceDirect Snapshot:C\:\\Users\\Amanda\\Zotero\\storage\\DE6792DP\\S0306454998000814.html:text/html},
